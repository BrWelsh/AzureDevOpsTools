<Window x:Class="AzureDevOpsTools.Presentation.Views.Dialogs.AboutDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AzureDevOpsTools.Presentation.Views.Dialogs"
        mc:Ignorable="d"
        Title="{Binding Path=Title, StringFormat=About: {0}}" Height="420" Width="620"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterOwner"
        SizeToContent="WidthAndHeight"
        ShowInTaskbar="False"
        x:Name ="dialog">
    <Grid/>
</Window>
